{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/include",
                "${workspaceFolder}/db",
                "${workspaceFolder}/port", 
                "${workspaceFolder}/util",
                "${workspaceFolder}/table",
                "${workspaceFolder}/helpers",
                "${workspaceFolder}/third_party/googletest/googletest/include",
                "${workspaceFolder}/third_party/googletest/googlemock/include",
                "/usr/include",
                "/usr/local/include",
                "/usr/include/c++/9",
                "/usr/include/x86_64-linux-gnu/c++/9",
                "/usr/include/c++/9/backward"
            ],
            "defines": [
                "LEVELDB_PLATFORM_POSIX=1",
                "_GNU_SOURCE",
                "__STDC_FORMAT_MACROS",
                "NDEBUG"
            ],
            "compilerPath": "/usr/bin/g++",
            "cStandard": "c17",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-gcc-x64",
            "browse": {
                "path": [
                    "${workspaceFolder}/**"
                ],
                "limitSymbolsToIncludedHeaders": false,
                "databaseFilename": "${workspaceFolder}/.vscode/browse.vc.db"
            },
            "compilerArgs": [
                "-Wall",
                "-Wextra",
                "-Wpedantic"
            ]
        }
    ],
    "version": 4
}
